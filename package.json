{
  "name": "alfalab-api",
  "version": "0.0.0",
  "description": "TypeScript + Mongoose + MongoDB + Express API Server",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "14.17.0"
  },
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/src/server.js",
    "dev": "npm run build && cross-env NODE_ENV=development nodemon --trace",
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit --detectOpenHandles --passWithNoTests --config ./jest.config.js",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint --fix",
    "heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "aws-sdk": "^2.953.0",
    "babel-node": "0.0.1-security",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "envalid": "^7.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^4.7.0",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "react-aws-s3": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.1.1",
    "underscore": "^1.13.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.17.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "node-gyp": "^8.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "_moduleAliases": {
    "@config": "dist/src/config",
    "@constants": "dist/src/constants",
    "@controllers": "dist/src/controllers",
    "@database": "dist/src/database",
    "@dtos": "dist/src/dtos",
    "@exceptions": "dist/src/exceptions",
    "@interfaces": "dist/src/interfaces",
    "@logs": "dist/src/logs",
    "@middlewares": "dist/src/middlewares",
    "@models": "dist/src/models",
    "@routes": "dist/src/routes",
    "@services": "dist/src/services",
    "@tests": "dist/src/test",
    "@utils": "dist/src/utils"
  }
}
